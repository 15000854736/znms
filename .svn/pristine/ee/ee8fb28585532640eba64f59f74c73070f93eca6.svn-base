@charset "utf-8";
/* CSS Document */
.s_student_accessbox{ width:100%; display:inline-block; text-align:center;vertical-align: top;padding:20px 0;}
.s_student_accessbox h1{ color:#0ce;font-size: 14px;font-weight: normal;text-align: center; padding:20px 0 10px 0; clear:both; }
.s_student_access_ulicon{ text-align:center;}
.s_student_data{display:inline-table;}
.s_student_a{color:#c3c;}
.s_student_b{color:#f36;}
.s_student_c{color:#09f;}
.s_student_d{ color:#96f;}
.s_student_ulicon{ display: inline-block; width:60%; text-align:right;  }
.s_student_ulicon li{ display: inline-block;width:33%;vertical-align: top;
text-align:right; margin:5%;}
.s_student_databg{ margin:1% 0 0 0;margin-left: 20px;width:350px;height: 150px}
.s_student_data{ background: url(../../../../images/screen/module/studentAccessType/8.png) no-repeat  center/100%; width:150px; height:120px; position:relative;display: inline-block; float:left;}
.s_student_no_a,.s_student_no_b,.s_student_no_c,.s_student_no_d{ position:absolute; font-size:14px; font-weight:bold;}
.s_student_no_a{ left:45px;color:#f36;margin-left: -15px}
.s_student_no_b{ left:117px;top:20px;color:#c3c;}
.s_student_no_c{left:0px;top:40px;color:#96f;}
.s_student_no_d{ left:155px;top:60px;color:#09f;}
.s_student_access_center{ height:130px;}
