@charset "utf-8";
body{ width:1366px; height:768px;font-size:12px;}
.prj_logo{ height:35px; padding:2px;}
.prj_logo>img{max-width:279px;max-height:41px;}
.prj_top{ height:342px;}
.line_box{width:6px; height:345px; margin:0 -6px; }
.line_bottom{ width:100%; height:24px; }

.online_box{ width:500px;margin:0 0 0 15px;}
.online_turntabl_box{width:240px; height:240px;margin:30px  0 0 0; }
.online_turntabl_box p{ font-size:80px;}
.online_turntabl_box svg{ height:76px;}
.online_turntabl_box h1{ font-size:14px;padding:70px 0 0 0;}
.online_turntable_1{ width:240px; height:240px;top:0px;left:0px;}

.online_people{width:240px;}
.online_people li{ width:85px; height:125px;line-height:30px; margin:10px;}
.online_people_circle{width:85px;height:125px;}
.online_people canvas{top:-20px !important;}
.numbe_a,.active_b,.free_c,.scour_d{ height:23px;width:100%;line-height:23px; }

.online_devices{ margin:8px 0 0 ;}
.online_devices li{margin:0 5px;}
.mobile_devices,.wireless_devices,.wired_devices,.other_devices{ width:130px; height:30px; line-height:30px; padding:0 5px;}
.online_devices,.mobile_devices,.wired_devices,.other_devices{font-size:12px;}
.online_devices li i{ width:30px; height:35px; }
.online_devices li span{font-size:14px; }
.mobile_devices i{ background-position:-15px -61px;}
.wireless_devices i{ background-position:-9px -113px;}
.wired_devices i{ background-position:-9px -9px;}
.other_devices i{ background-position:-9px -164px;}

.diagram_box{width:450px; padding:15px; height:300px;padding-top:25px !important;}

.network_health{ margin:35px 0 0 20px; width:275px; height:275px;}
.network_health h1{ font-size:14px; padding:65px 0 0 0;}
.network_health span{ font-size:110px;}

.prj_bottom{ height:330px;}
.export_utilization{ width:225px; margin:0 15px;}
.scale_diagram_b>div{height:110px;width:100%;}
.export_utilization dt{ padding:5px 0; font-size:16px; }

.traffic_data ul li h2{ padding:4px 0; font-size:12px;}
.traffic_data li{ margin:10px 30px;}
.traffic_data li p{ font-size:16px; }
.traffic_data>ul{margin-bottom:7px;}

.zlog_box{ width:42%; }
.zlog_box h1{ font-size:16px; padding:5px 0 5px 0;}
.zlog_data_a{ width:270px; height:165px;}
.zlog_data_b{padding-top:13px;}
.zlog_data_b li{ margin:0 12px;}
.zlog_data_list{ width:248px;}
.zlog_data_list li{ height:19px; margin:5px 0;}
.data_img_box{ height:3px; border-radius:5px;}
.percentage_box{ height:3px;}
.zlog_chart{height:110px;width:150px;}

.pre_menu_box{ padding:0 0 0 30px; border-radius:0 0 0 50px;}
.pre_menu_box a{padding:10px ; }
.pre_menu_box li{margin:0 0 0 10px; }

.card-holder { width: 0px; right:50px;}
.card {  width:600px;left: 30px; }
.card:hover {margin-left: -550px;}

.doughnut_container {height: 163px;}
.doughnut_container svg {left: 40px;top: -20px;}
#url_ranking svg {left: 5px;top: -75px}
#left_down {	width: 57.3%;}
.zlog_data_list>li {margin-bottom: 15px;margin-top: 7px;}

.scale_diagram_a {height:125px;}

.zlog_title{top:-26px;height:30px;line-height:30px;font-size:12px;}
.zlog_title>span{font-size:16px !important;}