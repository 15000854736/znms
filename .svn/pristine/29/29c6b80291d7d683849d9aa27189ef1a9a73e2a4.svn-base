.net_monitor_no{}
.net_monitor_hold{ }
.net_monitor_restore{  }
.net_monitor_modhov{ min-width:250px; min-height:120px; position:absolute; top:50px; left:0; padding:10px; color:#000; z-index:9999; border-radius:5px; color:#fff; background:#000; opacity:0.8; }
.net_monitor_modhov ul li{  padding:3px 0; }

.but_mod_top a{ display:block; border:1px solid #29d; color:#29d; padding:5px; border-radius:3px;}
.but_mod_top a:hover{ background:#428bca; color:#fff; }

.prompt_fontbox{ margin:10px; border:1px solid #f60; color:#f60; padding:5px 10px; text-align:center; display:inline-table;}
.email_box_yes{ width:500px; white-space: pre-wrap; word-wrap:break-word; padding:30px; text-align: left;}
.bg_color_box{ background:#fff; color:#000; padding:10px; margin:10px 0; overflow:auto; height:200px;}
.font_no_text{ font-size:24px; color:#555; text-align:center;}
.icon_no_font{ color:#555; font-size:60px; text-align:center;}

.znms_online{ font-family:impact; font-size:50px; text-align:center; margin:25% 0 0 0;}
.znms_online_ul li{ display:inline-table; margin:25px 10px; line-height:20px; font-size:12px;}
.znms_online_ul li p{font-size:12px;}
.data_list_ul li a{  padding:5px 5px; border-bottom:1px dotted #2f5089; font-size:14px; cursor:pointer; display:block; margin:0 5px; text-align:left;}

.statistics_box{ float:right; overflow:hidden; min-width:500px;}
.statistics_box .select2-choice{ float:right;    padding:6px 25px 0px 15px; height:32px;}
.statistics_box .input-sm{height:28px; background:#fff;}
.statistics_box  .select2-arrow {top:35%;}

.full_but{ position:absolute; right:10px; top:10px;}
.full_but:hover{ opacity:0.6;filter:alpha(opacity=60);}

.sta_top ul li{ display:inline-table; min-width:25%; height:20px; border-radius:5px;  font-family:impact !important; font-size:40px; margin:0 5px; color:#2db48c;}
.sta_top  p{ font-size:12px;}
.sta_bottom{ margin:30px 0 0 0;}

.log_statistics_box{ clear:both; padding:15px; overflow:hidden;}
.log_statistics_chart{float:left; width:50%;height:250px;padding: 15px 0 0 0; }
.log_statistics_list{width:50%;float:left;height:250px; overflow:auto;padding: 25px 0 0 0; }
.log_statistics_list ul{ padding:0 15px; width:75% ; float:right; }
.log_statistics_list ul li{ text-align:left; margin:20px 0; font-size:14px; }
.log_statistics_list ul i{ width:20px; height:20px; line-height:20px; background:#29d; border-radius:3px; color:#fff; font-style:normal; text-align:center;  margin:0 5px; vertical-align:middle;}

.config_comparison_box{ width:100%; border-collapse:collapse; float:left; margin:30px 0;  border-left: 1px solid #ccc;}
.config_comparison_box td,.config_comparison_box th{ text-align:center; width:50%; padding:10px; border-bottom:1px solid #ccc;border-right:1px solid #ccc;word-wrap:break-word; word-break:break-all; }
.config_comparison_box th{ background:#29d; color:#fff; border-right:1px solid #fff;}

.config_comparison_bg{ background:#c9e8f8 !important; font-weight:bold;}
.bg_yellow{ background:yellow;}

.topn_mod_box{ width:45%; display:inline-table;text-align:center;  }
.topn_mod_ul{ padding:40px 0 0;  display:inline-table; width:100%; }
.topn_mod_ul li{word-break:break-all; width:19%; display:inline-table; margin:0 0; text-align: center; height:180px;position:relative; }
.topn_font{ position:absolute;top:170px;text-align:center; width:90%;}
.topn_number{ bottom:30px;color:#555; line-height:20px; width:40px; border-radius:5px; position:absolute; left:50%; margin:0 0 0 -20px; bottom:20px;    }
.topn_schedule_cpu .topn_number{ background:url(../images/topncpuimg.png) repeat-x bottom;}
.topn_schedule_ram .topn_number{ background:url(../images/topnramimg.png) repeat-x bottom;}
.topn_number_font{ margin:-20px 0 0 0; }






