@charset "utf-8";
/* CSS Document */
.f_flow_box{ width:100%; overflow:hidden;  display:inline-block; ext-align:center;vertical-align: top;padding:20px 0; height: 450px;}
.f_flow_box h1{ color:#0ce;font-size: 14px;font-weight: normal;text-align: center; padding:20px 0 10px 0; clear:both; }

.f_total_flow{ color:#fff; width:250px; height:250px; float:left; text-align:center; vertical-align:middle; position:relative; }
.f_marginbox{ padding:38% 0 0 0;}
.f_data_bgimg{ position:absolute; top:0; left:0;}
.f_box_internet_traffic{ display:inline-table; width:50%; margin:0 0 0 2%;}
.f_device_a i,.f_device_b i,.f_device_c i,.f_device_d i{ background:url(../images/f_iconimg.png) no-repeat ; width:25px; height:25px;margin: 0 0 -17px 0;}
.f_device_a i{background-position:8px 0;}
.f_device_b i{background-position:-42px 0;}
.f_device_c i{background-position:-86px 0;}
.f_device_d i{background-position:-136px 0;}
.f_device_a{ color:#0cc;}
.f_device_b{color:#09c;}
.f_device_c{color:#6c3;}
.f_device_d{color:#f93;}
.f_daily_internet h2,.f_times_internet h2{ font-size:12px; color:#0ce; font-weight:normal; text-align:center; margin:1% 0;}
.f_device_ul { text-align:center;}
.f_device_ul li{ display:inline-table; margin: 0px 4% 5px 3px; }
.f_device_ul li span{ margin:0 0 0 10px; font-weight:bold; font-size:14px;margin: -3px 0 0 27px; display:block; }
.f_total_flow{ margin:1% 0 0 0;}
.f_device_ul_b li span{ margin:0; font-size:22px; display:inline-block;	}
.f_device_ul_b li {width:14%;}

.f_device_data{width:500px;height: 90px;margin-left:20px;}
.f_zlog_data_a svg {margin-left: -21px;;margin-top: -8px ;width:320px;height:320px; }
.f_cylinder_data svg{margin-left: -31px;margin-top: -50px;height: 150px;width: 500px; margin-bottom: -15px;}
