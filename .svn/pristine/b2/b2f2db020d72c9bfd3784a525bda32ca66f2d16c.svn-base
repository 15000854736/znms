@charset "utf-8";
/* CSS Document */
.a_access_timebox{ width:100%; display:inline-block; text-align:center;vertical-align: top;height: 362px}
.a_access_timebox h1{ color:#0ce;font-size: 14px;font-weight: normal;text-align: center; padding:5px 0 10px 0; clear:both; }
.a_everyday_data{ display: inline-block; text-align:left;}
.a_access_ulicon{ width:56%; float:right; text-align:center;}
.a_access_ulicon  li i{ width:35px; height:35px;}
.a_access_ulicon li{ float:left; width:50%; margin:3% 0;}
.a_access_a i,.a_access_b i,.a_access_c i,.a_access_d i{ background:url(../images/a_everydayicon.png) no-repeat;}
.a_access_a i{ background-position:0 0;}
.a_access_b i{ background-position:-90px 0;}
.a_access_c i{ background-position:-179px 0;}
.a_access_d i{ background-position:-269px 0;}
.a_access_a{color:#fc0;}
.a_access_b{color:#f60; }
.a_access_c{color:#6c3; }
.a_access_d{ color:#0ff;}
.a_each_data{ text-align:center;}
.a_each_ulicon{ width:85%;     margin: 0 30px 0 0;}
.a_each_ulicon li{ width:25%; display:inline-table;}

.a_zlog_data_a svg{margin-left: -25px;margin-top: -30px; margin-bottom: -35px;}
.every_timr_analysis svg{margin-left: 5px;margin-top: -44px; margin-bottom: -20px;}

.zlog_box{ width:100%; float:left;}
.zlog_box h1{color:#0ce; font-size:18px; font-weight:normal; text-align:center; padding:20px 0 10px 0;}
.zlog_flow_data_a{ float: left; text-align:center; width:240px; height:180px;}
.zlog_flow_data_a svg{margin-left: -45px;margin-top: -50px;}
.zlog_chart{height:128px;width:180px;}
.zlog_data_list{}
.flowCharts{width:420px;height:90px;}
.zlog_data_b{ clear:both;}
.zlog_data_b li{ display:inline-table; text-align:center; margin:0 30px;}
.zlogdata_num{ float:right;color:#0ce;}
.zlogdata_url{ float:left;color:#0ce;}
.zlog_data_list{ width:190px; float:right;}
.zlog_data_list li{ position:relative; height:20px; margin:5px 0;}
.data_img_box{ width:100%;background:#669; height:3px; border-radius:5px; position:absolute; bottom:0;}
.percentage_box{ height:3px;position:absolute; z-index:999;}
.warn_1{ background:#3cc}.font_warn_1{ color:#fff;}
.warn_2{ background: #390;}
.warn_3{ background: #f90;}
.warn_4{ background: #C03;}
.warn_5{ background: #f60;}
.warn_6{ background: #939;}
.warn_7{ background: #09f;}
.warn_8{ background: #F93;}
.warn_9{ background: #f20;}
.warn_10{ background: #0C0;}


