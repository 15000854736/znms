			if($("#exportDiv").children().length<3){
				return;
			}
			$("#exportDiv").animate({"margin-left":"-300px"}, "normal", "linear", function(){
				$("#exportDiv dl:eq(0)").appendTo($("#exportDiv"));
				$("#exportDiv").css("margin-left","0px");
			});			
		}, 6000)
	});

