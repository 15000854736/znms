@charset "utf-8";
/* CSS Document */
.f_flow_box{ width:100%; overflow:hidden;  display:inline-block; text-align:center;vertical-align: top; height: 500px;}
.f_flow_box h1{ color:#0ce;font-size: 18px;font-weight: normal;text-align: center; padding: 20px 0 10px 0; clear:both; }

.f_total_flow{ color:#fff; width:390px; height:290px; float:left; text-align:center; vertical-align:middle; position:relative; }
.f_marginbox{ padding:38% 0 0 0;}
.f_data_bgimg{ position:absolute; top:0; left:0;}
.f_box_internet_traffic{ display:inline-table; width:60%; margin:0 0 0 2%;}
.f_device_a i,.f_device_b i,.f_device_c i,.f_device_d i{ background:url(../../../../images/screen/module/f_iconimg.png) no-repeat ; width:25px; height:25px;}
.f_device_a i{background-position:8px 0;}
.f_device_b i{background-position:-42px 0;}
.f_device_c i{background-position:-86px 0;}
.f_device_d i{background-position:-136px 0;}
.f_device_a{ color:#0cc;}
.f_device_b{color:#09c;}
.f_device_c{color:#6c3;}
.f_device_d{color:#f93;}
.f_daily_internet h2,.f_times_internet h2{ font-size:12px; color:#0ce; font-weight:normal; text-align:center; margin:1% 0;}
.f_device_ul { text-align:center; margin: 0 40px 0px 0px;width: 800px;height: 25px;}
.f_device_ul li{ display:inline-table; margin:0 1%; }
.f_device_ul li span{ margin:0 0 0 10px; font-weight:bold; font-size:14px; }
.f_total_flow{ margin:3% 0 0 0;}
.f_device_ul_b li span{ margin:0; font-size:28px; display:inline-block;	}
.f_device_ul_b li {width:21%;}

.f_device_data{width:800px;height: 152px;margin-left:0px;}
.f_zlog_data_a svg{margin-left: -30px;margin-top: -40px;}
.f_cylinder_data svg{margin-left: -82px;margin-top: -24px;}