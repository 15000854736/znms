.screen_grid{
    display:table;
    width:100%;
}
.screen_grid > div >div{
    width:32%;
    height: auto;
    margin:10px;
    padding: 15px;
    border-radius: 5px;
    position: relative;
    box-shadow: 1px 1px 1px 1px #b4cadf;
    float:left;
}
.R{
    background: url(../../images/bg_img.png) repeat;
    cursor:pointer;
    position:absolute;
    right:0px;
    top:0px;
}

.grid-stack {
    /*background: lightgoldenrodyellow;*/
}

.grid-stack-item-content {
    /*color: #2c3e50;
    text-align: center;
    background-color: #18bc9c;*/
}
.screen_logo{
    text-align: center;
    max-height: 50px;
}
.close{ color:#555 !important; float:right; cursor:pointer; position:absolute; right:5px; top:5px;}
.close span:hover{ opacity:0.7;filter:alpha(opacity=70);}
.nms_box{ margin:0 0 0 170px; padding:60px 10px 0 10px; }
.big_screen_ul li{ position:relative; float:left; border:1px solid #ccc; padding:5px; margin:1% 0 0 1%; width:32%; height:310px; overflow: hidden; border-radius:5px; text-align:center;}
.big_screen_ul li h1{ font-size:14px; font-weight:normal; text-align:center; height:30px; line-height:30px;}
.big_screen_img img{ width:95%;  margin:1%;}
.big_screen_menu{ position:absolute; right:10px;top:5px; }
.full_screen_,.edit_img,.del_img{ background:url(../../images/bigmenuimg.png) no-repeat; width:25px; height:25px; margin:0 0 0 10px; cursor:pointer;}
.full_screen{ background-position:-5px 0 ;}
.edit_img{ background-position:-41px 0 ;}
.del_img{ background-position:-80px 0 ;}
.big_screen_menu i:hover{ opacity:0.7;}
.bk_a{ border:1px solid #ccc;}
.selinput_doc{ padding:5px; width:100%;}
@media screen and (max-width:1366px) {
.big_screen_ul li{ position:relative; float:left; border:1px solid #ccc; padding:5px; margin:1% 0 0 1%; width:32%; height:280px; overflow: hidden; border-radius:5px; text-align:center;}
}