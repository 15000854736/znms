.equipment_box{ padding:10px 0 0 0 ; height:180px;}
.equipment_box li{ color:#3f6 ; width:130px; height:180px; display:inline-block; text-align:center; margin:0 5px;border:1px solid #3f6; overflow:hidden}
.downtime{ border:1px solid #f06;}
.equipment_iconbox{color:#3f6;position:relative; width:111px; height:111px; margin:10px ; background:url(../../../../images/screen/module/index/monitoringiconbg_b.png) no-repeat center;}
.equipment_iconbox i{ width:76px; height:57px; margin:25px 15px;}
.normal_bg i{ background:url(../../../../images/screen/module/index/monitoringicon.png) no-repeat -166px 0;}
.downtime_bg i{ background:url(../../../../images/screen/module/index/monitoringicon.png) no-repeat 0 0;}
.downtime_bg{ border:1px solid #f06 !important; color:#f06 !important;}
.downtime_bg .equipment_iconbga{ background:url(../../../../images/screen/module/index/monitoringiconbgb.png) no-repeat center;}
.downtime_bg .equipment_iconbox{ background:url(../../../../images/screen/module/index/monitoringiconbg_c.png) no-repeat center;}
.equipment_iconbga{ top:0; left:0; position:absolute; width:111px; height:111px; background:url(../../../../images/screen/module/index/monitoringiconbg.png) no-repeat center;}


.equipment_iconbga{
			-webkit-animation-name: spin; 
			-webkit-animation-duration:2000ms; /* 40 seconds */
			-webkit-animation-iteration-count: infinite; 
			-webkit-animation-timing-function: linear;
			
			-moz-animation-name: spin; 
			-moz-animation-duration:2000ms; /* 40 seconds */
			-moz-animation-iteration-count: infinite; 
			-moz-animation-timing-function: linear;
			
			-ms-animation-name: spin; 
			-ms-animation-duration:2000ms; /* 40 seconds */
			-ms-animation-iteration-count: infinite; 
			-ms-animation-timing-function: linear;
			/* boooo opera */
			-o-transition: rotate(3600deg); /* works */
}
@-webkit-keyframes spin {from {-webkit-transform: rotate(0deg);}to {-webkit-transform: rotate(360deg);}}
@-moz-keyframes spin {from {-moz-transform: rotate(0deg);}to {-moz-transform: rotate(360deg);}}
@-ms-keyframes spin {from {-ms-transform: rotate(0deg);}to {-ms-transform: rotate(360deg);}}