@charset "utf-8";
/* CSS Document */
.a_access_timebox{ width:100%; display:inline-block; text-align:center;vertical-align: top;height: 428px;}
.a_access_timebox h1{ color:#0ce;font-size: 14px;font-weight: normal;text-align: center; padding:10px 0 10px 0; clear:both; }
.a_everyday_data{ display: inline-block; text-align:left;}
.a_access_ulicon{ width:56%; float:right; text-align:center;}
.a_access_ulicon  li i{ width:35px; height:35px;}
.a_access_ulicon li{ float:left; width:50%; margin:3% 0;}
.a_access_a i,.a_access_b i,.a_access_c i,.a_access_d i{ background:url(../images/a_everydayicon.png) no-repeat;}
.a_access_a i{ background-position:0 0;}
.a_access_b i{ background-position:-90px 0;}
.a_access_c i{ background-position:-179px 0;}
.a_access_d i{ background-position:-269px 0;}
.a_access_a{color:#0c9;  }
.a_access_b{color:#0cc; }
.a_access_c{color:#6c3; }
.a_access_d{ color:#09c;}
.a_each_data{ text-align:center;}
.a_each_ulicon{ width:190%;  margin: 0 35px 0 0;}
.a_each_ulicon li{ width:25%; display:inline-table;}

.zlog_data_a svg {margin-left: -3px;margin-top:-10px;width:215px;height:140px;}