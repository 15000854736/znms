package info.zznet.znms.base.entity;

public class ScreenConfig {
    private String screenId;

    private Integer screenRow;	
    
    private Integer screenCol;	
    
    private Integer screenWidth;

    private Integer screenHeight;
    
    private String screenConfigData;
    
    
    public String getScreenId() {
        return screenId;
    }

    public void setScreenId(String screenId) {
        this.screenId = screenId;
    }

    public Integer getScreenWidth() {
        return screenWidth;
    }

    public void setScreenWidth(Integer screenWidth) {
        this.screenWidth = screenWidth;
    }

    public Integer getScreenHeight() {
        return screenHeight;
    }

    public void setScreenHeight(Integer screenHeight) {
        this.screenHeight = screenHeight;
    }

	public String getScreenConfigData() {
		return screenConfigData;
	}

	public void setScreenConfigData(String screenConfigData) {
		this.screenConfigData = screenConfigData;
	}

	public Integer getScreenRow() {
		return screenRow;
	}

	public void setScreenRow(Integer screenRow) {
		this.screenRow = screenRow;
	}

	public Integer getScreenCol() {
		return screenCol;
	}

	public void setScreenCol(Integer screenCol) {
		this.screenCol = screenCol;
	}
}